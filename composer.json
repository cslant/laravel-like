{
  "name": "cslant/laravel-like",
  "description": "A package providing üëç like and üëé dislike features for Laravel applications.",
  "keywords": [
    "cslant",
    "laravel",
    "cslant-laravel-like",
    "cslant-like",
    "laravel-like",
    "laravel-dislike",
    "cslant-package",
    "like-dislike",
    "like-package",
    "laravel-like-package"
  ],
  "homepage": "https://github.com/cslant/laravel-like",
  "license": "MIT",
  "authors": [
    {
      "name": "Tan Nguyen",
      "email": "tannp27@gmail.com",
      "homepage": "https://tanhongit.com",
      "role": "Maintainer"
    }
  ],
  "autoload": {
    "psr-4": {
      "CSlant\\Laravel\\Like\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CSlant\\Laravel\\Like\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.1"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^v3.0",
    "nunomaduro/collision": "^7.10",
    "nunomaduro/larastan": "^2.9",
    "orchestra/testbench": "^8.0",
    "pestphp/pest": "^2.0",
    "phpstan/extension-installer": "^1.3",
    "phpstan/phpstan-deprecation-rules": "^1.1",
    "phpstan/phpstan-phpunit": "^1.3"
  },
  "scripts": {
    "analyse": "vendor/bin/phpstan analyse",
    "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes",
    "test": "vendor/bin/pest"
  },
  "support": {
    "issues": "https://github.com/cslant/laravel-like/issues"
  },
  "extra": {
    "laravel": {
      "providers": [
        "CSlant\\Laravel\\Like\\Providers\\LikeServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "phpstan/extension-installer": true,
      "php-http/discovery": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
